{
  "name": "@nuxtjs/firebase",
  "version": "7.6.1",
  "description": "Intergrate Firebase into your Nuxt project.",
  "keywords": [
    "firebase",
    "firestore",
    "google",
    "googleauthentication",
    "nuxt",
    "realtimedatabase"
  ],
  "homepage": "https://firebase.nuxtjs.org/",
  "repository": "nuxt-community/firebase-module",
  "license": "MIT",
  "author": "Pascal Luther",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "main": "lib/module.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.vue lib test",
    "release": "standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && jest",
    "commit": "npx git-cz",
    "docs": "cd docs && yarn dev"
  },
  "dependencies": {
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.12",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@nuxt/types": "^2.15.3",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "codecov": "latest",
    "core-js": "^3.14.0",
    "cz-conventional-changelog": "latest",
    "eslint": "^7.22.0",
    "firebase": "^8.3.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "nuxt": "^2.15.3",
    "proxy-mock-js": "^1.0.3",
    "standard-version": "^9.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.1"
  },
  "peerDependencies": {
    "firebase": "^8.3.1",
    "nuxt": "^2.15.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalDependencies": {
    "firebase-admin": "^9.9.0"
  }
}
